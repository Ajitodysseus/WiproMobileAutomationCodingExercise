import org.openqa.selenium.support.FindBy;

public class CardDetails 
{
	
	@FindBy(xpath="//*[contains(text(), 'Search')]")
	private WebElement Srch;
	
	
	public void SearchItems(String Itm)
	{
		Srch.sendKeys(Itm);
		btn.click();
		
	}

}
